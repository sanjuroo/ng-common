{
    "name": "@ng2/common",
    "version": "2.1.0",
    "description": "Angular 2 module for common stuff",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts":
    {
        "build": "npm run tsc",
        "clean": "rimraf dist",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "webpack",
        "bundle:min": "webpack --env.minify",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:min",
        "deploy:stable": "npp -b",
        "deploy:next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "repository":
    {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/angular2-utilities.git"
    },
    "keywords": [
        "angular2",
        "ng2",
        "utilities"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs":
    {
        "url": "https://gitlab.com/Buko/angular2-utilities/issues"
    },
    "homepage": "https://gitlab.com/Buko/angular2-utilities#README",
    "dependencies":
    {
        "@angular/core":  "~2.2.0",
        "@angular/common":  "~2.2.0",
        "@angular/compiler":  "~2.2.0",
        "@angular/platform-browser": "~2.2.0",
        "@angular/platform-browser-dynamic": "~2.2.0",
        "@angular/forms": "~2.2.0",
        "@angular/router":  "~3.2.0",
        "core-js": "~2.4.1",
        "reflect-metadata": "~0.1.8",
        "zone.js": "~0.6.25",
        "rxjs": "5.0.0-beta.12"
    },
    "peerDependencies":
    {
        "numeral": "~1.5.3"
    },
    "devDependencies":
    {
        "@types/numeral": "~0.0.18",
        "rimraf": "~2.5.4",
        "typescript": "2.0.9",
        "webpack": "2.1.0-beta.25",
        "npm-publish-package": "~1.2.0"
    }
}